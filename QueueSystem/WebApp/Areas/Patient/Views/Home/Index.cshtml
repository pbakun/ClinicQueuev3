@model List<int>
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="container">
    <div class="btn-group btn-group-lg" role="group">

        <a class="btn btn-info btn-lg" asp-area="Doctor" asp-controller="Doctor" asp-action="Index">Doctor</a>
        @if (Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <a class="btn btn-dark btn-lg" href="./patient/@item.ToString()">Pok. @item.ToString()</a>
            }

        }


    </div>

</div>
